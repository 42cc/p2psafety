server {
        listen   80;

        server_name <%= @project %>;

        location / {
                uwsgi_pass unix:/tmp/uwsgi/<%= @project %>.sock;
                include uwsgi_params;
        }

        location /static/ {
                alias /home/vagrant/www/<%= @project %>/static/;
        }

        location /media/ {
                alias /home/vagrant/www/<%= @project %>/media/;
        }
}
