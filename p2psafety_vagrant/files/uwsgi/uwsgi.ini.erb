[uwsgi]
# Define variables
project = %n


# Config
uid = <%= @uwsgi_user %>
gid = <%= @uwsgi_group %>

socket = /tmp/%(project).sock
master = true
no-orphans = true

chdir = /home/vagrant/<%= @django_path %> 
module = %(project).wsgi:application
virtualenv = /home/vagrant/virtualenvs/<%= @project %>/

logto = /var/log/uwsgi/%(project).log
